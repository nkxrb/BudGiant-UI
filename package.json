{
  "name": "budgiant-ui",
  "version": "0.0.1",
  "description": "A vue.js 3.0 UI for web/app",
  "main": "dist/budgiant/budgiant-ui.js",
  "unpkg": "dist/budgiant/budgiant-ui.min.js",
  "module": "dist/budgiant/budgiant-ui.esm.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "node scripts/build.js vue -f global -d && jest --runInBand",
    "test-dts": "node scripts/build.js shared reactivity runtime-core runtime-dom -dt -f esm-bundler && yarn test-dts-only",
    "test-dts-only": "tsc -p ./test-dts/tsconfig.json && tsc -p ./test-dts/tsconfig.build.json",
    "dev": "serve & open http://localhost:5000/examples/index.html",
    "build": "node ./scripts/build.rollup.js"
  },
  "prepublishOnly": "npm run build",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nkxrb/BudGiant-UI.git"
  },
  "keywords": [
    "BudGiant",
    "UI"
  ],
  "author": "nkxrb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nkxrb/BudGiant-UI/issues"
  },
  "homepage": "https://github.com/nkxrb/BudGiant-UI#readme",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/helper-module-imports": "^7.10.4",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "autoprefixer": "^9.8.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "cssnano": "^4.1.10",
    "jest": "^26.1.0",
    "rollup": "^2.22.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^3.1.3",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^6.0.0-beta.9",
    "serve": "^11.3.2",
    "vue": "^3.0.0-rc.2"
  }
}